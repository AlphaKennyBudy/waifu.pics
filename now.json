{
    "version": 2,
    "builds": [
        {
            "src": "index.js",
            "use": "@now/node-server"
        }
    ],
    "routes": [
        {
            "src": "/.*",
            "headers": {
                "cache-control": "no-cache"
            },
            "dest": "index.js"
        }
    ]
}