{
    "version": 2,
    "builds": [
        {
            "src": "index.js",
            "use": "@now/node-server"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "https://waifu.pics/api/$1",
            "headers": {
                "cache-control": "no-cache"
            }
        }
    ]
}